{
    "version": 2,
    "builds": [
      {
        "src": "api/flask_handler.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/flask_handler.py"
      }
    ]
  }